{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "jsx-double", "avoid-escape", "avoid-template"],
        "trailing-comma": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false
    },
    "linterOptions": {
        "exclude": [
            "src/serviceWorker.ts"
        ]
    },
    "rulesDirectory": []
}
